<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="personalProfileVM" type="com.jamessapplication.app.modules.personalprofile.data.viewmodel.PersonalProfileVM"/>
    </data>
    <FrameLayout android:id="@+id/framePersonalProfil"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStyleindigo_50_cf">
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_6pxv"
            android:layout_marginEnd="@dimen/_14pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_9pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_21pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="@dimen/_25pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft_amber_a700"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_amber_a700"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtVerify"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_181pxh"
                        android:layout_weight="0.82"
                        android:layout_width="0dp"
                        android:paddingBottom="@dimen/_1pxv"
                        android:paddingEnd="@dimen/_5pxh"
                        android:paddingStart="@dimen/_5pxh"
                        android:paddingTop="@dimen/_1pxv"
                        android:singleLine="true"
                        android:text="@{personalProfileVM.personalProfileModel.txtVerify}"
                        style="@style/txtSolidRoundedOutline" tools:text="@string/lbl_delete_account"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnellipsesix"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_2pxh"
                android:layout_marginStart="@dimen/_3pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyleorange_1">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowellipsesix"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_21pxh"
                    android:layout_marginStart="@dimen/_21pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <View
                        android:background="@drawable/rectangle_bg_bluegray_900_5e_radius_44_81"
                        android:id="@+id/viewEllipseSix"
                        android:layout_height="@dimen/_84pxv"
                        android:layout_marginBottom="@dimen/_1pxv" android:layout_width="@dimen/_89pxh"/>
                    <ImageButton
                        android:background="@drawable/rectangle_bg_pink_700_0c_radius_12_565"
                        android:id="@+id/btnEdit"
                        android:layout_height="@dimen/_25pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_58pxv"
                        android:layout_width="@dimen/_25pxh"
                        android:paddingHorizontal="@dimen/_5pxh"
                        android:paddingVertical="@dimen/_5pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_edit_light_blue_900" tools:ignore="ContentDescription"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtEditimage"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_70pxv"
                        android:layout_weight="0.29"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{personalProfileVM.personalProfileModel.txtEditimage}"
                        style="@style/txtMitrlight9" tools:text="@string/lbl_edit_image"/>
                </LinearLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:gravity="center_horizontal"
                    android:id="@+id/recyclerListname"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_9pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_10pxh}" tools:itemCount="2" tools:listitem="@layout/row_listname"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtHeight"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_9pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{personalProfileVM.personalProfileModel.txtHeight}"
                    style="@style/txtMitrlight9" tools:text="@string/lbl_height"/>
                <RadioButton android:ellipsize="end"
                    android:id="@+id/rbRowclock"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_21pxh"
                    android:layout_marginStart="@dimen/_21pxh"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_genders"
                    android:textColor="@color/black_900"
                    android:textSize="@dimen/_8pxh" tools:text="@string/lbl_genders"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowverifyOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_36pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtVerifyOne"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_2pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_2pxv"
                    android:singleLine="true"
                    android:text="@{personalProfileVM.personalProfileModel.txtVerifyOne}"
                    style="@style/txtSolidRoundedOutline_1" tools:text="@string/lbl_edit_details"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtVerifyTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_29pxh"
                    android:layout_weight="0.5"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_2pxv"
                    android:paddingEnd="@dimen/_4pxh"
                    android:paddingStart="@dimen/_4pxh"
                    android:paddingTop="@dimen/_6pxv"
                    android:singleLine="true"
                    android:text="@{personalProfileVM.personalProfileModel.txtVerifyTwo}"
                    style="@style/txtSolidRoundedOutline_2" tools:text="@string/msg_add_emergency_c"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
