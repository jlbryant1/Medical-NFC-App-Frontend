<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="personalProfileVM" type="com.jamessapplication.app.modules.personalprofile.data.viewmodel.PersonalProfileVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearPersonalProfil"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleindigo_50_cf">
        <ImageView android:id="@+id/imageArrowleft"
            android:layout_height="@dimen/_19pxh"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_width="@dimen/_30pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_arrowleft"
            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumn"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_6pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_21pxh"
            android:layout_marginTop="@dimen/_21pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearColumnellipsesix"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_3pxh"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyleorange_1">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowellipsesix"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_21pxh"
                    android:layout_marginStart="@dimen/_21pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <View
                        android:background="@drawable/rectangle_bg_bluegray_900_5e_radius_44_81"
                        android:id="@+id/viewEllipseSix"
                        android:layout_height="@dimen/_84pxv"
                        android:layout_marginBottom="@dimen/_1pxv" android:layout_width="@dimen/_89pxh"/>
                    <ImageButton
                        android:background="@drawable/rectangle_bg_pink_700_0c_radius_12_565"
                        android:id="@+id/btnFrame150"
                        android:layout_height="@dimen/_25pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_58pxv"
                        android:layout_width="@dimen/_25pxh"
                        android:paddingHorizontal="@dimen/_5pxh"
                        android:paddingVertical="@dimen/_5pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_edit_light_blue_900" tools:ignore="ContentDescription"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtEditimage"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_70pxv"
                        android:layout_weight="0.29"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{personalProfileVM.personalProfileModel.txtEditimage}"
                        style="@style/txtMitrlight9" tools:text="@string/lbl_edit_image"/>
                </LinearLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:gravity="center_horizontal"
                    android:id="@+id/recyclerListname"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_9pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_10pxh}" tools:itemCount="2" tools:listitem="@layout/row_listname"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtHeight"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_9pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{personalProfileVM.personalProfileModel.txtHeight}"
                    style="@style/txtMitrlight9" tools:text="@string/lbl_height"/>
                <LinearLayout android:id="@+id/linearRowgender"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_28pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtGender"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_weight="0.11"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{personalProfileVM.personalProfileModel.txtGender}"
                        style="@style/txtMitrlight9" tools:text="@string/lbl_gender"/>
                    <FrameLayout android:id="@+id/frameStacktwentyseven"
                        android:layout_height="@dimen/_24pxv"
                        android:layout_marginStart="@dimen/_9pxh" android:layout_width="@dimen/_253pxh">
                        <EditText
                            android:background="@android:color/transparent"
                            android:ellipsize="end"
                            android:hint="@string/lbl_male_or_female"
                            android:id="@+id/etTwentySeven"
                            android:inputType="text"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@={personalProfileVM.personalProfileModel.etTwentySevenValue}"
                            android:textColorHint="@color/black_900"
                            style="@style/etMitrlight14" tools:ignore="Autofill"/>
                        <View android:background="@color/cyan_700"
                            android:id="@+id/lineLineTwentyOneTwo"
                            android:layout_gravity="bottom|start"
                            android:layout_height="@dimen/_1pxv"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_253pxh"/>
                    </FrameLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowverify"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_29pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtVerify"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_2pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_2pxv"
                    android:singleLine="true"
                    android:text="@{personalProfileVM.personalProfileModel.txtVerify}"
                    style="@style/txtSolidRoundedOutline" tools:text="@string/lbl_edit_details"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtVerifyOne"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_29pxh"
                    android:layout_weight="0.5"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_2pxv"
                    android:paddingEnd="@dimen/_4pxh"
                    android:paddingStart="@dimen/_4pxh"
                    android:paddingTop="@dimen/_6pxv"
                    android:singleLine="true"
                    android:text="@{personalProfileVM.personalProfileModel.txtVerifyOne}"
                    style="@style/txtSolidRoundedOutline_1" tools:text="@string/msg_add_emergency_c"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
