<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="breathingRateRowModel" type="com.jamessapplication.app.modules.breathingrate.data.model.BreathingRateRowModel"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumntoday"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleindigo_1">
        <TextView android:ellipsize="end" android:id="@+id/txtToday"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_155pxh"
            android:layout_marginStart="@dimen/_155pxh"
            android:layout_marginTop="@dimen/_9pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{breathingRateRowModel.txtToday}"
            style="@style/txtNunitoSansbold10_1" tools:text="@string/lbl_today"/>
        <LinearLayout android:id="@+id/linearRowtime"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_8pxh"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylewhite_1">
            <LinearLayout android:id="@+id/linearRowgrouptwentyfour"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_6pxv"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_weight="0.69" android:layout_width="0dp" android:orientation="horizontal">
                <ImageView android:id="@+id/imageGroupTwentyFour"
                    android:layout_height="@dimen/_11pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_11pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group24"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group24"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtOneHundredOne"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_weight="0.76"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{breathingRateRowModel.txtOneHundredOne}"
                    android:textAllCaps="true"
                    style="@style/txtNunitoSansbold7" tools:text="@string/lbl_1003"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTimeThree"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_8pxv"
                android:layout_marginEnd="@dimen/_8pxh"
                android:layout_marginStart="@dimen/_229pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_weight="0.31" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{breathingRateRowModel.txtTimeThree}"
                style="@style/txtNunitoSansregular9_1" tools:text="@string/lbl_14_06"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearRowtimeOne"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="@dimen/_8pxh"
            android:layout_marginVertical="@dimen/_8pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylewhite_1">
            <LinearLayout android:id="@+id/linearRowtwitter"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_6pxv"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_weight="0.65" android:layout_width="0dp" android:orientation="horizontal">
                <ImageView android:id="@+id/imageTwitter"
                    android:layout_height="@dimen/_11pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_11pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_twitter"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_twitter"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNinetyThree"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_weight="0.71"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{breathingRateRowModel.txtNinetyThree}"
                    android:textAllCaps="true"
                    style="@style/txtNunitoSansbold7" tools:text="@string/lbl_93"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTimeOne"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_8pxv"
                android:layout_marginEnd="@dimen/_8pxh"
                android:layout_marginStart="@dimen/_238pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_weight="0.35" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{breathingRateRowModel.txtTimeOne}"
                style="@style/txtNunitoSansregular9_1" tools:text="@string/lbl_14_06"/>
        </LinearLayout>
    </LinearLayout>
</layout>
