<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="skinTempRowModel" type="com.jamessapplication.app.modules.skintemp.data.model.SkinTempRowModel"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumntoday"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleindigo_52cornerRadius">
        <TextView android:ellipsize="end" android:id="@+id/txtToday"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_164pxh"
            android:layout_marginStart="@dimen/_164pxh"
            android:layout_marginTop="@dimen/_9pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{skinTempRowModel.txtToday}"
            style="@style/txtNunitoSansbold10" tools:text="@string/lbl_today"/>
        <LinearLayout android:id="@+id/linearRowtime"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_9pxh"
            android:layout_marginStart="@dimen/_9pxh"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylewhite_A700cornerRadius">
            <LinearLayout android:id="@+id/linearRowgrouptwentythree"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_4pxv"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_weight="0.74" android:layout_width="0dp" android:orientation="horizontal">
                <ImageView android:id="@+id/imageGroupTwentyThree"
                    android:layout_height="@dimen/_11pxh"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="@dimen/_9pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group23"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group23"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNineHundredFiftyFive"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_weight="0.68"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{skinTempRowModel.txtNineHundredFiftyFive}"
                    android:textAllCaps="true"
                    style="@style/txtNunitoSansbold16" tools:text="@string/lbl_95_5"/>
                <TextView android:ellipsize="end" android:id="@+id/txtF"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_weight="0.12"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{skinTempRowModel.txtF}"
                    style="@style/txtNunitoSansregular10" tools:text="@string/lbl_f"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTimeThree"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_8pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_225pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_weight="0.26" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{skinTempRowModel.txtTimeThree}"
                style="@style/txtNunitoSansregular9_1" tools:text="@string/lbl_14_06"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearRowtimeOne"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_9pxv"
            android:layout_marginEnd="@dimen/_9pxh"
            android:layout_marginStart="@dimen/_9pxh"
            android:layout_marginTop="@dimen/_8pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylewhite_A700cornerRadius">
            <LinearLayout android:id="@+id/linearRowgrouptwentythreeOne"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_4pxv"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_weight="0.74" android:layout_width="0dp" android:orientation="horizontal">
                <ImageView android:id="@+id/imageGroupTwentyThreeOne"
                    android:layout_height="@dimen/_11pxh"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="@dimen/_9pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group23"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group23"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNineHundredFiftyFiveOne"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_weight="0.68"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{skinTempRowModel.txtNineHundredFiftyFiveOne}"
                    android:textAllCaps="true"
                    style="@style/txtNunitoSansbold16" tools:text="@string/lbl_95_5"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_weight="0.12"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{skinTempRowModel.txtFOne}"
                    style="@style/txtNunitoSansregular10" tools:text="@string/lbl_f"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTimeOne"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_8pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_225pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_weight="0.26" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{skinTempRowModel.txtTimeOne}"
                style="@style/txtNunitoSansregular9_1" tools:text="@string/lbl_14_06"/>
        </LinearLayout>
    </LinearLayout>
</layout>
